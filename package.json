{
  "name": "simple-cache-ts",
  "version": "1.0.0",
  "description": "A lightweight, type-safe, in-memory cache with TTL and multiple eviction policies",
  "author": "Shreedhar Hegde",
  "repository": {
    "type": "git",
    "url": "https://github.com/shreedhar-hegde/simple-cache.git"
  },
  "homepage": "https://github.com/shreedhar-hegde/simple-cache#readme",
  "bugs": {
    "url": "https://github.com/shreedhar-hegde/simple-cache/issues"
  },
  "keywords": [
    "cache",
    "memory-cache",
    "lru",
    "ttl",
    "typescript",
    "zero-dependencies"
  ],
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "scripts": {
    "dev": "tsx src/index.ts",
    "build": "tsup src/index.ts --format cjs,esm --dts --out-dir dist --cjs-interop",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "eslint . --ext .ts --fix",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "prepare": "husky"
  },
  "sideEffects": false,
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^24.2.1",
    "@typescript-eslint/eslint-plugin": "8.39.1",
    "@typescript-eslint/parser": "8.39.1",
    "@vitest/coverage-v8": "^3.2.4",
    "eslint": "^9.33.0",
    "eslint-config-prettier": "^8.9.0",
    "eslint-define-config": "^2.1.0",
    "eslint-plugin-prettier": "^5.5.4",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5",
    "prettier": "^3.6.2",
    "ts-node": "^10.9.2",
    "tsup": "^8.5.0",
    "tsx": "^4.20.4",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ]
  },
  "dependencies": {
    "typescript-eslint": "^8.39.1"
  }
}